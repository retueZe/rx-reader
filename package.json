{
  "private": true,
  "name": "rx-reader",
  "version": "1.0.0-b.2",
  "author": "retueze",
  "license": "MIT",
  "description": "Fast async streamed I/O library based on RxJS and generators.",
  "keywords": [
    "async",
    "stream",
    "streaming",
    "streamed",
    "io",
    "reader",
    "rxjs",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/retueZe/rx-reader"
  },
  "files": [
    "dist",
    "operators",
    "utils"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./operators": {
      "import": "./dist/operators.mjs",
      "require": "./dist/operators.cjs",
      "types": "./dist/operators.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "build:prod": "npm run lint && rollup -c",
    "build:dev": "tsc --outDir dist --inlineSourceMap --module commonjs",
    "clean": "rm -rf dist operators utils",
    "prepare": "npm run clean && npm run build:prod",
    "postpublish": "npm run clean"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "rollup": "^3.21.2",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "async-option": "^2.5.0"
  }
}
